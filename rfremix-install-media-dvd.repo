[rfremix-install-media-dvd]
name=RFRemix Install Media DVD - $basearch
failovermethod=priority
baseurl=file:///media/RFRemix%2015%20$basearch%20DVD/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-fixes-fedora
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-free-fedora
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-russianfedora-nonfree-fedora
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-fedora
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-free-fedora
cost=2000
